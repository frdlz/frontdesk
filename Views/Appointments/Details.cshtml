@model Frontdesk6.Models.Frontdesk.Appointment

@{
    ViewData["Title"] = "Details";
    Layout = null;
}
<head>
    <link href="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>

    <script src="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/lib/jquery-datetimepicker/jquery.datetimepicker.css" />
    <style>
        
        dd{
            text-align:left
        }
        .rght{
            text-align:right
        }
    </style>

</head>
<div class="container">


    <div class="card bg-light border-dark mt-4">
        <div class="card-header">
            <img src="~/img/logo.png" style="height:80px" class="float-sm-right mt-0 pt-0" />
            <h5 class="mt-4">Balai KIPM Balikpapan</h5>
        </div>
        <div class="card-body rounded">
            

            <div>
                <h5>Layanan Konsultasi</h5>
                <hr />

                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.NomorApp)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.NomorApp)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Tanggal)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Tanggal)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Subject)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Subject)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Deskripsi)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Deskripsi)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.NamaLayanan)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.NamaLayanan)
                    </dd>

                </dl>
            </div>
            
        </div>
        <div class="card-footer h-100">
            <div class="row">
                <div class="col-md-6 ">
                    Cek jumlah antrian anda di <a asp-action="Count">Sini</a>
                </div>
                
            </div>
            
        </div>
    </div>
    <button onclick="window.print()">Print</button>
    <a asp-action="Create">Back</a>
</div>