@model IEnumerable<Frontdesk6.Models.Frontdesk.Appointment>

@{
    ViewData["Title"] = "Index";
    Layout = null;
}
<head>
    <link href="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>

    <script src="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/lib/jquery-datetimepicker/jquery.datetimepicker.css" />
    <!--FontAwesome-->
    <link href="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet">
    
    <style>
        .buttonfx {
            width: 60px;
        }
    </style>

</head>
<div class="container mt-2">
 

   


            

           <div>
               
                   
                   <form asp-action="Index4" method="get">
                       <div class="form-actions form-inline no-color border-dark ml-3">
                           <div class="row">
                               <input placeholder="Search" class="form-control mb-2 mr-sm-2" type="text" name="SearchString" value="@ViewData["CurrentFilter"]" />
                               <button type="submit" value="Search" class="btn btn-info buttonfx mb-2 mr-sm-2"><i class="fas fa-search"></i></button>
                               <a class="btn btn-warning buttonfx mb-2 text-center" asp-action="Index4"><i class="fas fa-1x fa-undo mt-1"></i></a>
                           </div>
                       </div>
                   </form>
               
               <div>
                   <table class="table">
                       <thead>
                           <tr>
                               <th>

                                   @Html.DisplayNameFor(model => model.NomorApp)
                               </th>
                               <th>
                                   @Html.DisplayNameFor(model => model.Tanggal)
                               </th>
                               <th>
                                   @Html.DisplayNameFor(model => model.Subject)
                               </th>
                               <th>
                                   @Html.DisplayNameFor(model => model.Deskripsi)
                               </th>
                               <th>
                                   @Html.DisplayNameFor(model => model.NamaLayanan)
                               </th>



                               <th></th>
                           </tr>
                       </thead>
                       <tbody>
                           @foreach (var item in Model)
                           {
                               <tr>
                                   <td>

                                       @Html.DisplayFor(modelItem => item.NomorApp)
                                   </td>
                                   <td>
                                       @Html.DisplayFor(modelItem => item.Tanggal)
                                   </td>
                                   <td>
                                       @Html.DisplayFor(modelItem => item.Subject)
                                   </td>
                                   <td>
                                       @Html.DisplayFor(modelItem => item.Deskripsi)
                                   </td>
                                   <td>
                                       @Html.DisplayFor(modelItem => item.NamaLayanan)
                                   </td>
                                   <td>
                                   <td>

                                       <a asp-action="Details" asp-route-id="@item.AppointmentID">Details</a> |

                                   </td>


                               </tr>
                           }
                       </tbody>
                   </table>
               </div>
           </div>
            
           
        </div>
   