@model Frontdesk6.Models.Frontdesk.Appointment

@{
    ViewData["Title"] = "Proses";
    Layout = "_LayoutLTEAppoint";
}
<head>
    <link href="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>

    <script src="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/lib/jquery-datetimepicker/jquery.datetimepicker.css" />
    <style>

        dd {
            text-align: left
        }

        .rght {
            text-align: right
        }
    </style>

</head>
<div class="container mt-1">


    <div class="card">
        <div class="card-body">


            <h1>Proses</h1>


            <hr />
            <div class="row">
                <div class="col-md-12">
                    <form asp-action="Proses">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <input type="hidden" asp-for="AppointmentID" />
                        <input type="hidden" name="returnUrl" value="@ViewBag.returnUrl" />
                        <div class="form-group">
                            <label asp-for="NomorApp" class="control-label"></label>
                            <input disabled asp-for="NomorApp" class="form-control" />
                            <span asp-validation-for="NomorApp" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Tanggal" class="control-label"></label>
                            <input disabled type="text" asp-for="Tanggal" class="form-control" />
                            <span asp-validation-for="Tanggal" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Subject" class="control-label"></label>
                            <input disabled asp-for="Subject" class="form-control" />
                            <span asp-validation-for="Subject" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Deskripsi" class="control-label"></label>
                            <input disabled asp-for="Deskripsi" class="form-control" />
                            <span asp-validation-for="Deskripsi" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="NamaLayanan" class="control-label"></label>
                            <input disabled asp-for="NamaLayanan" class="form-control" />
                            <span asp-validation-for="NamaLayanan" class="text-danger"></span>
                        </div>

                        <div class="form-group">
                            <input type="submit" value="Save" class="btn btn-primary" />
                        </div>
                    </form>
                </div>
            </div>

            <div>
                <a asp-action="Index">Back to List</a>
            </div>
        </div>
    </div>
</div>
